{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/guardar-resultado",
            "dest": "server/index.js"
        },
        {
            "src": "/ranking",
            "dest": "server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}